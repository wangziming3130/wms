{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{url}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 19000
        }
      ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "UpstreamPathTemplate": "/system/api/{url}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "Key": "system_Key"
    },
    {
      "DownstreamPathTemplate": "/swagger",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 19000
        }
      ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "UpstreamPathTemplate": "/system/swagger",
      "UpstreamHttpMethod": [ "Get" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "Key": "system_swagger_key"
    },
    {
      "DownstreamPathTemplate": "/swagger/{url}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 19000
        }
      ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "UpstreamPathTemplate": "/system/swagger/{url}",
      "UpstreamHttpMethod": [ "Get" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "Key": "system_swagger_url_key"
    }
  ]
}
